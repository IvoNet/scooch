{
  "name": "scooch",
  "version": "3.0.0",
  "description": "Scooch - Presenting smarter",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/IvoNet/scooch.git"
  },
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "keywords": [
    "slides, presentations, slideshows, keynote, powerpoint, scooch"
  ],
  "author": {
    "name": "Ivo Woltring",
    "email": "webmaster@ivonet.nl",
    "url": "http://www.ivonet.nl"
  },
  "contributors": [
    {
      "name": "Philippe Tjon-A-Hen",
      "url": "https://github.com/pnmtjonahen"
    },
    {
      "name": "Dirk Luijk",
      "url": "https://github.com/dirkluijk"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "fs-walk": "0.0.1",
    "highlight.js": "^9.3.0",
    "mathjax": "^2.6.1",
    "reveal.js": "^3.2.0"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.3.0",
    "@builder.io/qwik-city": "^1.3.0",
    "@types/eslint": "^8.44.8",
    "@types/node": "^20.10.3",
    "@types/reveal.js": "^4.4.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-plugin-qwik": "^1.3.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "undici": "^5.26.0",
    "vite": "^5.0.6",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
